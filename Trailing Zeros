#include<bits/stdc++.h>
using namespace std;
#define ll long long
ll div(ll n){
    ll i = 0;
    while(n > 0 && n % 5 == 0){
        n = n / 5;
        i++;
    }
    return i;
}
void search(ll n){
    ll c = 0;
    for(ll x = n; x > 0; ){
        if(x % 5 == 0){
            //cout<<x<<"\n";
            c += div(x);
            x = x - 5;
        }
        else
            x--;
    }
    cout<<c;
}
int main(){
    ios::sync_with_stdio(0);
    cin.tie(0);
    ll n;
    cin>>n;
    search(n);
    return 0;
}
