#include<bits/stdc++.h>
using namespace std;
 
int main(){
    long long n, i;
    cin>>n;
    if(n == 1){
        cout<<n;
        return 0;
    }
    if(n < 4){
        printf("NO SOLUTION");
        return 0;
    }
    for(i = 2; i <= n; i = i + 2)
        cout<<i<<" ";
    for(i = 1; i <= n; i = i + 2)
        cout<<i<<" ";
    return 0;
}
